{
  "name": "ovh-ui-kit-documentation",
  "version": "2.13.0",
  "description": "The documentation and showcase application for OVH UI Kit.",
  "license": "BSD-3-Clause",
  "scripts": {
    "commit": "npm-scripts-config commit",
    "commitmsg": "npm-scripts-config commitmsg",
    "preversion": "yarn upgrade ovh-ui-kit ovh-ui-angular --latest && yarn build && git add yarn.lock",
    "version": "npm-scripts-config version",
    "postversion": "git push && git push --tags",
    "preview-changelog": "npm-scripts-config preview-changelog",
    "start": "cross-env NODE_ENV=dev babel-node ./build/server",
    "serve:dist": "opn http://localhost:8080 && static dist",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color --hide-modules --config ./build/webpack.dist.config.babel.js",
    "test": "echo \"no tests for this project\""
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^1.0.0-rc.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.17.0",
    "eslint-config-ovh": "^0.1.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.6.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "highlightjs": "^9.8.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.4",
    "lodash-webpack-plugin": "^0.11.4",
    "marked": "^0.3.6",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.6.1",
    "node-static": "^0.7.9",
    "npm-scripts-config": "^0.0.2",
    "npm-scripts-conventional-changelog": "^0.1.0",
    "opn": "^5.0.0",
    "opn-cli": "^3.1.0",
    "portscanner": "^2.1.1",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^4.1.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.16",
    "angular": "^1.6.1",
    "angular-aria": "^1.6.1",
    "angular-messages": "^1.6.1",
    "angular-sanitize": "^1.6.1",
    "flatpickr": "4.4.4",
    "lodash": "^4.17.4",
    "ovh-documentation-toolkit": "^1.0.0",
    "ovh-ui-angular": "git+https://github.com/ovh-ux/ovh-ui-angular#feature\/oui-button-guide",
    "ovh-ui-kit": "git+https://github.com/ovh-ux/ovh-ui-kit#feature\/oui-button-guide"
  }
}
